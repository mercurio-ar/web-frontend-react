{
  "name": "@mercurio-ar/web-frontend-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.1.1",
    "axios": "^0.18.0",
    "querystring": "^0.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-localize-redux": "^3.4.1",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "2.16.0",
    "recharts": "^1.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "lint": "tslint --fix ./src",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/node": "^10.11.3",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "@types/react-redux": "^6.0.9",
    "@types/recharts": "^1.1.0",
    "@types/redux": "^3.6.31",
    "@types/redux-thunk": "^2.1.32",
    "@types/reselect": "^2.2.0",
    "eslint": "^5.6.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.0.1",
    "jshint": "^2.9.6",
    "lint-staged": "^7.3.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test -- --forceExit --coverage --bail"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
